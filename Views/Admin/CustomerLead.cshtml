
@{
    ViewData["Title"] = "CustomerLead";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@*<h4>Customer Lead</h4>*@
<section class="content">
    <div class="container-fluid">


        <form>
            <div class="row">

                <div class="col-md-12">

                    <div class="card card-warning">
                        <div class="card-header">
                            <h3 class="card-title">Customer Lead</h3>
                        </div>

                        <div class="card-body ">

                            <div class="row">



                                <div class="col-sm-3">

                                    <div class="form-group">
                                        <label>Partner ID :	 </label>
                                        <input type="text" class="form-control" placeholder="Enter Partner ID" id="txtpartnerid" />

                                    </div>
                                </div>


                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label>
                                            Name<span style="color:red;">*</span>:
                                        </label>
                                        <input type="text" class="form-control" placeholder="Enter Name" id="txtname" />
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label>
                                            Customer Mobile<span style="color:red;">*</span>:
                                        </label>
                                        <input type="number" class="form-control" placeholder="Enter Customer Mobile" id="txtmobile" />
                                    </div>
                                </div> 
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label>
                                            Address:
                                        </label>
                                        <input type="text" class="form-control" placeholder="Enter Address" id="txtaddress" />
                                    </div>
                                </div>
                                 <div class="col-sm-3">
                                    <div class="form-group">
                                        <label>
                                            Customer Income:
                                        </label>
                                        <input type="text" class="form-control" placeholder="Enter Customer Income" id="txtincome" />
                                    </div>
                                </div>
                                 <div class="col-sm-3">
                                    <div class="form-group">
                                        <label>
                                            Customer Service:
                                        </label>
                                        <input type="text" class="form-control" placeholder="Enter Customer Service" id="txtservice" />
                                    </div>
                                </div>
                                

                            </div>

                          
                            <button type="button" id="btnSearch" class="btn btn-primary waves-effect waves-light" onclick="SaveDetails()">SAVE </button>




                        </div>

                    </div>
                </div>
            </div>

        </form>


    </div>

</section>
