
@{
    ViewData["Title"] = "GetUserMemberDocuments";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



@model DataTable
@using System.Data


<style>
	h3 {
		display: inline;
	}

	thead {
		font-size: 12px;
	}

	.card {
		border-radius: 10px;
        box-shadow: 0px 2px 5px 0px rgba(19, 23, 38, 0.05);
        margin-bottom: 30px !important;
        padding: 17px;
    }

    .lable {
        font-weight: bold;
    }
</style>



<div class="col-md-12">

	<div class="card card-warning ">

		<div class="row">
			<div class="col-md-12"><h4 class="new-title">Member Documents List</h4></div>
			<div class="col-sm-2">
				<label>Type</label>
				<select id="ddltype" onchange="userByType()" class="form-control">
					<option value="">All</option>
					<option value="BS">Business Startup</option>
					<option value="ITR">Income Tax Returns (ITR)</option>
					<option value="CR">Company Registration</option>
					<option value="TNR">Trust/NGO/Registration</option>
					<option value="TR">Trade Mark Registration</option>
					<option value="MSUR">MSME/Udayan Registration</option>
					<option value="CL">Contractor Licesce</option>
					<option value="FSSAI">FSSAI License</option>
					<option value="PSARA">PSARA License</option>
					<option value="CMA">CMA Report</option>
					<option value="ACC">Accounting</option>
					<option value="GRR">GST Registration & Return</option>
					<option value="PFLR">Partnership Firm/LLP Registration</option>
					<option value="SR">Shop Registration</option>
					<option value="PFESI">PF/ESI Registration</option>
					<option value="IEL">Import/Export License</option>
					<option value="RERA">RERA License</option>
					<option value="PROJR">Project Report</option>
					<option value="HOMEL">Home Loan</option>
					<option value="BUSIL">Business Loan</option>
					<option value="PERSOL">Personal Loan</option>
					<option value="PROPERL">Loan Against Property</option>
					<option value="CCAPPLY">Credit Card Apply</option>
					<option value="MFUNDS">Mutual Fund</option>
					<option value="FDEPOSIT">Fixed Deposit</option>
					<option value="NPSCHEME">National Pension Scheme (NPS)</option>
					<option value="BONDSDEB">Bond/ Debentures</option>
					<option value="SGBONDS">Sovereign Gold Bond</option>
					<option value="REIT">REIT</option>
					<option value="DEMAT">Dematerialize Share certificate</option>
				</select>
			</div>
			<div class="col-sm-2">
				<div class="form-group">
					<label>From Date</label>
					<input type="date" id="txtFromDate" class="form-control" />
				</div>
			</div>
			<div class="col-sm-2">
				<div class="form-group">
					<label>To Date</label>
					<input type="date" id="txtTodate" class="form-control" />
				</div>
			</div>
			<div class="col-sm-2">
				<div class="form-group pt-4">
					<input type="button" value="search" onclick="searchRecord()" class="btn btn-primary">
				</div>
			</div>
		</div>

	</div>
	<div class="card card-warning" id="outerdv">

		<div style="overflow-x:auto;" class="p-0" id="outertbl">
			<table id="example3" class="my   table table-striped  table-bordered table-hover ">
				<thead class="bg-primary">
					<tr>
						@* <th>Sr.No.</th>*@
						<th style="width: 60px;">Action</th>
						<th>Partner Id</th>
						<th>Name</th>
						<th>Mobile</th>
						<th>Email</th>
						@*<th>Service Enquery</th>*@
						<th>Doc Type</th>
                        <th>Close Date</th>
                        <th>Close Remarks</th>
						@*<th>Assign To</th>
						<th>Assign Date</th>
						<th>Progess</th>*@
						<th>EntryDate</th>
					</tr>
				</thead>
				<tbody>
					@{
						if (Model != null)
						{
							if (Model.Rows.Count > 0)
							{
								int i = 1;
								foreach (DataRow item in Model.Rows)
								{
									<tr>
										@*<td>@i</td>*@
										<td>
											<i class="fa fa-eye" style="font-size:25px;color:blue; cursor:pointer; padding-left: 10px;" onclick="ViewGetMemberDetails('@item["ID"]')"></i>
                                            @if (@item["CloseStatus"] == null || @item["CloseStatus"].ToString() == "")
                                            {
                                                //<i class="fa fa-trash" style="font-size:25px;color:red; cursor:pointer; padding-left: 10px;" aria-hidden="true" 
                                                //onclick="DeleteRecord('@item["ID"]')"></i>
                                                <i class="fa fa-edit" style="font-size:25px;color:green; cursor:pointer; padding-left: 10px;" onclick="Showmodel('@item["ID"]')"></i>
                                            }
                                            
										</td>
										<td>@item["Userid"]</td>
										<td>@item["Name"]</td>
										<td>@item["Mobileno"]</td>
										<td>@item["Email"]</td>
										@*<td></td>*@
										<td>@item["DocType"]</td>
                                        <td>@item["CloseDate"]</td>
                                        <td>@item["CloseRemarks"]</td>
										@*<td></td>
						<td></td>
						<td></td>*@
										<td>@item["EntryDates"]</td>
									</tr>
									i++;
								}
							}
						}
					}
				</tbody>
			</table>
		</div>
	</div>
</div>

