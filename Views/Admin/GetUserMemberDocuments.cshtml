			if (!(confirm("Do You want Delete  this Record  !!"))) {
				//location.reload();
				return false;

			}
			var objdata = {
				Id: Id
			}
			$.ajax({
				url: "/Admin/DocumentMemberDetailsDelete",
				type: "POST",
				data: objdata,
				success: function (r) {

					alert(r);
					window.location.reload(true);
				},
				error: function (XMLHttpRequest, textStatus, errorThrown) {
					alert('Please Check values Entered by you !!!');
					$("#showSpinner").hide();
				}

			});
		}

	</script>

