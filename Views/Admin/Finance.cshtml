<script>
    function openSetModal(url,txt,weburl)
    {
            $("#exampleModalLongTitle").text(txt)
            $("#auploadedocument").attr("href", url)
                            $("#aopenwebsite").attr("href",weburl)
                    $("#aopenwebsite").attr("target", "_blank")
    }
</script>       
