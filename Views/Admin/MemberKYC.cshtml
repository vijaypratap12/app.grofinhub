    }
    function userOnboarding(agentid, mobile) {

        var dataobj = { Mobile: mobile, Merchantcode: agentid, is_new: '0' };
        $.get("/Admin/UserOnbloarding", dataobj, function (res) {
            //var data=eval(res)

            console.log('onboarding ');
            console.log(res.msgbody);
            // $("#onboarding_modal_msg_p").text(res.message);
            // $("#open_redirect_url_onboarding_btn").hide();
            //$("#onboardingmodal").attr("style", "display:block;padding-right:17px;");
            //$("#onboardingmodal").attr("class", "modal fade show");
            if (res.status == "true" && res.response_code == "1") {
                //$("#onboarding_modal_form").attr("action", res.redirecturl)
                //$("#open_redirect_url_onboarding_btn").show();
