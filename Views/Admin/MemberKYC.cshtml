<script>


    @*Vishwajeet*@
        function Getagentdetails(id) {
            debugger;
            $("#txtparteridvj").val(id)
            var data = new FormData();
            data.append("Partnerid", id)

            $("#showSpinner").show();

            $.ajax({
                url: "/Admin/getagentkycdetails",
                type: 'POST',
                contentType: false,
                processData: false,
                data: data,
                success: function (s) {


                    if (s != '0') {
                        s = eval(s);
                        $.each(s, function (k, v) {
                            //$('input[name="txtparteridvj"]').val(v.PartnerId);
                            $('input[name="txtbankholdernamevj"]').val(v.BankHolderName);
                            $('input[name="txtbankaccountnovj"]').val(v.BankAccountNo);
                            $('input[name="txtifsccodevj"]').val(v.IFSCCode);
                            $('input[name="txtbanknamevj"]').val(v.BankName);
                            $('input[name="txtAadharvj"]').val(v.AadhaarNo);
                            $('input[name="txtPanvj"]').val(v.PanNo);
                        });
                    }
                }
            });
        }
    function updateKYCDetails() {
        debugger;
        var data = new FormData();
        data.append("Partnerid", $("#txtparteridvj").val())
        data.append("Bankname", $("#txtbanknamevj").val())
        data.append("Bankholdername", $("#txtbankholdernamevj").val())
        data.append("Bankaccountno", $("#txtbankaccountnovj").val())
        data.append("Ifsccode", $("#txtifsccodevj").val())
        data.append("AadhaarNo", $("#txtAadharvj").val())
        data.append("PanNo", $("#txtPanvj").val())

        $("#showSpinner").show();

        $.ajax({
            url: "/Admin/UpdateKYCDetails",
            type: "POST",
            contentType: false,
            processData: false,
            data: data,
            success: function (msg) {

                alert(msg);
                location.reload();

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert('Please Check values Entered by you !!!');
                $("#showSpinner").hide();
            }


        });

    }

    function deleteagentdetail(id) {
        debugger;
        var data = new FormData();
        data.append("Partnerid", id)

        $("#showSpinner").show();

        $.ajax({
            url: "/Admin/deletekycdetail",
            type: 'POST',
            contentType: false,
            processData: false,
            data: data,
            success: function (msg) {

                alert(msg);
                location.reload();

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert('Please Check values Entered by you !!!');
                $("#showSpinner").hide();
            }
        });
    }
    @*Vishwajeet*@



        function setParterId(id) {
            debugger;
            $("#txtparterid").val(id)
        }



    function saveKYCDetails() {
        debugger;
        var data = new FormData();
        data.append("Partnerid", $("#txtparterid").val())
        data.append("Bankname", $("#txtbankname").val())
        data.append("Bankholdername", $("#txtbankholdername").val())
        data.append("Bankaccountno", $("#txtbankaccountno").val())
        data.append("Ifsccode", $("#txtifsccode").val())
        //data.append("Aadharpicfront", $("#aadharpicfrontfile").get(0).files[0])
        //data.append("Aadharpicback", $("#aadharpicbackfile").get(0).files[0])
        //data.append("Panpicfront", $("#panpicfronfile").get(0).files[0])
        //data.append("Panpicback", $("#panpicbackfile").get(0).files[0])
        data.append("AadhaarNo", $("#txtAadhar").val())
        data.append("PanNo", $("#txtPan").val())

        $("#showSpinner").show();

        $.ajax({
            url: "/Admin/SaveUpdateKYCDetails",
            type: "POST",
            contentType: false,
            processData: false,
            data: data,
            success: function (msg) {

                alert(msg);
                location.reload();

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert('Please Check values Entered by you !!!');
