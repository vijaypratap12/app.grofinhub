                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">


                <div class="row m-2">
                    <div class="col-sm-4">
                        <label>Partner ID</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" id="txtparterid" readonly class="form-control" placeholder="Enter Partner Id">
                    </div>
                </div>
                <div class="row m-2">
                    <div class="col-sm-4">
                        <label>Account Holder Name</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" id="txtbankholdername" class="form-control" placeholder="Enter Bank Holder Name" onkeyup="this.value = this.value.toUpperCase();">
                    </div>
                </div>
                <div class="row m-2">
                    <div class="col-sm-4">
                        <label>Bank Account No.</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" id="txtbankaccountno" class="form-control" placeholder="Enter Bank Account No" oninput="this.value = this.value.replace(/[^0-9]/g, '').replace(/(\..*?)\..*/g, '$1');">
                    </div>
                </div>
                <div class="row m-2">
                    <div class="col-sm-4">
                        <label>IFSC Code</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" id="txtifsccode" class="form-control" placeholder="Enter Bank IFSC Code" onkeyup="this.value = this.value.toUpperCase();">
                    </div>
                </div>
                <div class="row m-2">
                    <div class="col-sm-4">
                        <label>Bank Name</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" id="txtbankname" onkeyup="this.value=this.value.toUpperCase();" class="form-control" placeholder="Enter Bank Name">
                    </div>
                </div>








